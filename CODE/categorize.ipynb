    "def preprocess_text(text):\n",
    "    text = re.sub(r'\\W+', ' ', text.lower())\n",
    "    words = [stemmer.stem(word) for word in text.split() if word not in stop_words]\n",
    "    return ' '.join(words)\n"
   ]
